{
    "name": "graham-campbell/logviewer",
    "description": "LogViewer Provides A LogViewer Admin Module For Laravel 5",
    "keywords": ["laravel", "framework", "log viewer", "LogViewer", "logviewer", "LogViewer", "Laravel LogViewer", "Laravel-LogViewer", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/filesystem": "5.2.*|5.3.*",
        "illuminate/pagination": "5.2.*|5.3.*",
        "illuminate/routing": "5.2.*|5.3.*",
        "illuminate/session": "5.2.*|5.3.*",
        "illuminate/support": "5.2.*|5.3.*",
        "illuminate/view": "5.2.*|5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~3.0",
        "phpunit/phpunit": "^4.7.6"
    },
    "suggest": {
        "graham-campbell/navigation": "Easily add a navigation bar."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\LogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\LogViewer\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
